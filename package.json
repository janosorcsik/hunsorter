{
	"name": "hunsorter",
	"version": "2.1.0",
	"description": "Sorting text with the Hungarian spelling rules",
	"keywords": [
		"hungarian",
		"sort",
		"typescript"
	],
	"homepage": "https://github.com/janosorcsik/hunsorter#readme",
	"bugs": {
		"url": "https://github.com/janosorcsik/hunsorter/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janosorcsik/hunsorter.git"
	},
	"license": "MIT",
	"author": "JÃ¡nos Orcsik",
	"funding": "https://github.com/janosorcsik/hunsorter?sponsor=1",
	"sideEffects": false,
	"source": "src/main.ts",
	"exports": {
		"require": "./dist/main.cjs",
		"default": "./dist/main.modern.js"
	},
	"main": "./dist/main.cjs",
	"module": "./dist/main.module.js",
	"unpkg": "./dist/main.umd.js",
	"types": "dist/main.d.ts",
	"files": [
		"dist/**.js*",
		"dist/**/*.ts"
	],
	"scripts": {
		"build": "microbundle",
		"format": "prettier --write \"**/*.{js,json,md,mjs,mts,ts,yml}\"",
		"lint": "eslint",
		"prepack": "rm -rf dist && yarn build",
		"release": "yarn prepack && np",
		"test": "vitest --run"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^8.7.0",
		"@typescript-eslint/parser": "^8.7.0",
		"eslint": "^9.11.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-xo-typescript": "^6.0.0",
		"eslint-plugin-prettier": "^5.2.1",
		"microbundle": "^0.15.1",
		"np": "^10.0.7",
		"prettier": "^3.3.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"typescript": "^5.6.2",
		"vite": "^5.4.8",
		"vitest": "^2.1.1"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	}
}
