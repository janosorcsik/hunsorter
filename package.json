{
	"name": "hunsorter",
	"version": "2.2.1",
	"description": "Sorting text with the Hungarian spelling rules",
	"keywords": [
		"hungarian",
		"sort",
		"typescript"
	],
	"homepage": "https://github.com/janosorcsik/hunsorter#readme",
	"bugs": {
		"url": "https://github.com/janosorcsik/hunsorter/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janosorcsik/hunsorter.git"
	},
	"license": "MIT",
	"author": "JÃ¡nos Orcsik",
	"funding": "https://github.com/janosorcsik/hunsorter?sponsor=1",
	"sideEffects": false,
	"source": "src/main.ts",
	"exports": {
		"require": "./dist/main.cjs",
		"default": "./dist/main.modern.js"
	},
	"main": "./dist/main.cjs",
	"module": "./dist/main.module.js",
	"unpkg": "./dist/main.umd.js",
	"types": "dist/main.d.ts",
	"type": "module",
	"files": [
		"dist/**.js*",
		"dist/**/*.ts"
	],
	"scripts": {
		"build": "microbundle",
		"format": "biome format --write",
		"lint": "biome lint",
		"prepack": "rm -rf dist && pnpm build",
		"release": "pnpm prepack && np",
		"test": "vitest --run"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.1",
		"microbundle": "^0.15.1",
		"np": "^10.2.0",
		"typescript": "^5.8.3",
		"vite": "^7.0.4",
		"vitest": "^3.2.4"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
