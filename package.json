{
	"name": "hunsorter",
	"description": "Sorting text with the Hungarian spelling rules",
	"version": "2.0.1",
	"source": "src/main.ts",
	"main": "dist/main.js",
	"exports": "./dist/main.modern.js",
	"module": "dist/main.module.js",
	"unpkg": "dist/main.umd.js",
	"types": "dist/main.d.ts",
	"sideEffects": false,
	"keywords": [
		"hungarian",
		"sort",
		"typescript"
	],
	"files": [
		"dist/**.js*",
		"dist/**/*.ts"
	],
	"scripts": {
		"build": "microbundle",
		"test": "ava",
		"lint": "xo src test",
		"format": "xo src test --fix",
		"prepack": "rm -rf dist && yarn build",
		"release": "yarn prepack && np"
	},
	"devDependencies": {
		"ava": "^3.15.0",
		"microbundle": "^0.13.0",
		"np": "^7.5.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.4",
		"xo": "^0.39.1"
	},
	"xo": {
		"prettier": true,
		"space": true
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register/transpile-only"
		]
	},
	"license": "MIT",
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/janosorcsik/hunsorter.git"
	},
	"author": "JÃ¡nos Orcsik",
	"bugs": {
		"url": "https://github.com/janosorcsik/hunsorter/issues"
	},
	"homepage": "https://github.com/janosorcsik/hunsorter#readme"
}
