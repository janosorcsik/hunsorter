{
	"name": "hunsorter",
	"version": "2.1.0",
	"description": "Sorting text with the Hungarian spelling rules",
	"keywords": [
		"hungarian",
		"sort",
		"typescript"
	],
	"homepage": "https://github.com/janosorcsik/hunsorter#readme",
	"bugs": {
		"url": "https://github.com/janosorcsik/hunsorter/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janosorcsik/hunsorter.git"
	},
	"license": "MIT",
	"author": "JÃ¡nos Orcsik",
	"funding": "https://github.com/janosorcsik/hunsorter?sponsor=1",
	"sideEffects": false,
	"source": "src/main.ts",
	"exports": {
		"require": "./dist/main.cjs",
		"default": "./dist/main.modern.js"
	},
	"main": "./dist/main.cjs",
	"module": "./dist/main.module.js",
	"unpkg": "./dist/main.umd.js",
	"types": "dist/main.d.ts",
	"files": [
		"dist/**.js*",
		"dist/**/*.ts"
	],
	"scripts": {
		"build": "microbundle",
		"format": "prettier --write \"**/*.{js,json,md,ts,yml}\"",
		"lint": "eslint \"**/*.ts\"",
		"prepack": "rm -rf dist && yarn build",
		"release": "yarn prepack && np",
		"test": "vitest --run"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.8.1",
 		"@typescript-eslint/parser": "^5.8.1",
 		"eslint": "^8.5.0",
 		"eslint-config-prettier": "^8.3.0",
 		"eslint-config-xo": "^0.39.0",
 		"eslint-config-xo-typescript": "0.48.0",
 		"eslint-plugin-prettier": "^4.0.0",
 		"microbundle": "^0.14.2",
 		"np": "^7.6.0",
 		"prettier": "^2.5.1",
 		"prettier-plugin-sorted": "^2.0.0",
 		"vite": "^2.7.10",
 		"vitest": "0.0.125",
 		"typescript": "^4.5.4"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	}
}
