{
	"name": "hunsorter",
	"version": "2.0.2",
	"description": "Sorting text with the Hungarian spelling rules",
	"keywords": [
		"hungarian",
		"sort",
		"typescript"
	],
	"homepage": "https://github.com/janosorcsik/hunsorter#readme",
	"bugs": {
		"url": "https://github.com/janosorcsik/hunsorter/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janosorcsik/hunsorter.git"
	},
	"license": "MIT",
	"author": "JÃ¡nos Orcsik",
	"funding": "https://github.com/janosorcsik/hunsorter?sponsor=1",
	"sideEffects": false,
	"main": "dist/main.umd.js",
	"unpkg": "dist/main.umd.js",
	"module": "dist/main.modern.module.js",
	"source": "src/main.ts",
	"types": "dist/main.d.ts",
	"files": [
		"dist/**.js*",
		"dist/**/*.ts"
	],
	"scripts": {
		"build": "microbundle",
		"format": "prettier --write \"**/*.{js,json,md,ts,yml}\"",
		"lint": "eslint \"**/*.ts\"",
		"prepack": "rm -rf dist && yarn build",
		"release": "yarn prepack && np",
		"test": "jest"
	},
	"devDependencies": {
		"@types/jest": "^27.0.2",
		"@typescript-eslint/eslint-plugin": "^4.31.2",
		"@typescript-eslint/parser": "^4.31.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-xo": "^0.39.0",
		"eslint-config-xo-typescript": "^0.45.1",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.2.1",
		"microbundle": "^0.14.0",
		"np": "^7.5.0",
		"prettier": "^2.4.1",
		"prettier-plugin-sorted": "^2.0.0",
		"ts-jest": "^27.0.5",
		"typescript": "^4.4.3"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	}
}
