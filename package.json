{
	"name": "hunsorter",
	"version": "2.2.1",
	"description": "Sorting text with the Hungarian spelling rules",
	"keywords": [
		"hungarian",
		"sort",
		"typescript"
	],
	"homepage": "https://github.com/janosorcsik/hunsorter#readme",
	"bugs": {
		"url": "https://github.com/janosorcsik/hunsorter/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janosorcsik/hunsorter.git"
	},
	"license": "MIT",
	"author": "JÃ¡nos Orcsik",
	"funding": "https://github.com/janosorcsik/hunsorter?sponsor=1",
	"sideEffects": false,
	"source": "src/main.ts",
	"exports": {
		"require": "./dist/main.cjs",
		"default": "./dist/main.modern.js"
	},
	"main": "./dist/main.cjs",
	"module": "./dist/main.module.js",
	"unpkg": "./dist/main.umd.js",
	"types": "dist/main.d.ts",
	"type": "module",
	"files": [
		"dist/**.js*",
		"dist/**/*.ts"
	],
	"scripts": {
		"build": "microbundle",
		"format": "biome format --write",
		"lint": "biome lint",
		"prepack": "rm -rf dist && pnpm build",
		"release": "pnpm prepack && np",
		"test": "vitest --run"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"microbundle": "^0.15.1",
		"np": "^10.2.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"vitest": "^3.2.4"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
