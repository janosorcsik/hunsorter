{
	"name": "hunsorter",
	"description": "Sort by Hungarian ABC",
	"version": "0.0.1",
	"source": "src/main.ts",
	"main": "dist/main.umd.js",
	"module": "dist/main.modern.module.js",
	"unpkg": "dist/main.umd.js",
	"types": "dist/main.d.ts",
	"keywords": [
		"hungarian",
		"sort",
		"typescript"
	],
	"files": [
		"dist/**.js*",
		"dist/**/*.ts"
	],
	"scripts": {
		"build": "microbundle",
		"test": "ava",
		"lint": "xo src test",
		"format": "xo src test --fix",
		"prepack": "rm -rf dist && yarn build",
		"release": "yarn prepack && np"
	},
	"devDependencies": {
		"ava": "^3.15.0",
		"microbundle": "^0.13.0",
		"np": "^7.2.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3",
		"xo": "^0.37.1"
	},
	"xo": {
		"prettier": true,
		"globals": [],
		"rules": {}
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register/transpile-only"
		]
	},
	"license": "MIT",
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"repository": {
    "type": "git",
    "url": "git+https://github.com/janosorcsik/hunsorters.git"
  },
  "author": "JÃ¡nos Orcsik",
  "bugs": {
    "url": "https://github.com/janosorcsik/hunsorters/issues"
  },
  "homepage": "https://github.com/janosorcsik/hunsorters#readme"
}
