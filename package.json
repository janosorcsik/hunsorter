{
	"name": "hunsorter",
	"version": "2.2.1",
	"description": "Sorting text with the Hungarian spelling rules",
	"keywords": [
		"hungarian",
		"sort",
		"typescript"
	],
	"homepage": "https://github.com/janosorcsik/hunsorter#readme",
	"bugs": {
		"url": "https://github.com/janosorcsik/hunsorter/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janosorcsik/hunsorter.git"
	},
	"license": "MIT",
	"author": "JÃ¡nos Orcsik",
	"funding": "https://github.com/janosorcsik/hunsorter?sponsor=1",
	"sideEffects": false,
	"source": "src/main.ts",
	"exports": {
		"require": "./dist/main.cjs",
		"default": "./dist/main.modern.js"
	},
	"main": "./dist/main.cjs",
	"module": "./dist/main.module.js",
	"unpkg": "./dist/main.umd.js",
	"types": "dist/main.d.ts",
	"type": "module",
	"files": [
		"dist/**.js*",
		"dist/**/*.ts"
	],
	"scripts": {
		"build": "microbundle",
		"format": "biome format --write",
		"lint": "biome lint",
		"prepack": "rm -rf dist && pnpm build",
		"release": "pnpm prepack && np",
		"test": "vitest --run"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.2",
		"microbundle": "^0.15.1",
		"np": "^10.2.0",
		"typescript": "^5.9.3",
		"vite": "^7.1.12",
		"vitest": "^3.2.4"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
